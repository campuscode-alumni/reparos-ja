<%= form_for([@contractor, @estimate]) do |f| %>
  <%= f.label :description, 'Descrição do serviço' %>
  <%= f.text_area :description %>
  <%= f.label :location, 'Local' %>
  <%= f.text_field :location %>
  <%= f.label :service_date, 'Data' %>
  <%= f.text_field :service_date %>
  <%= f.label :day_shift, 'Manhã' %>
  <%= f.check_box(:day_shift) %>
<% end %>